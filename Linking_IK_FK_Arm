connectAttr -f L_IK_Hip_Jnt.rotateZ L_Hip_Blend.color2B;

connectAttr -f L_Hip_Blend.outputR L_Bind_Hip_Jnt.rotateX;
connectAttr -f L_Hip_Blend.outputG L_Bind_Hip_Jnt.rotateY;
connectAttr -f L_Hip_Blend.outputB L_Bind_Hip_Jnt.rotateZ;

