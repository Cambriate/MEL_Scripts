//Link connections - HIP
connectAttr -f L_FK_Hip_Jnt.rotateX L_Hip_Blend.color1R;
connectAttr -f L_FK_Hip_Jnt.rotateY L_Hip_Blend.color1G;
connectAttr -f L_FK_Hip_Jnt.rotateZ L_Hip_Blend.color1B;

